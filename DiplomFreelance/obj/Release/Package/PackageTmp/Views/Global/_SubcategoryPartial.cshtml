@model IEnumerable<DiplomFreelance.Models.FreelanceModels.ViewModel.SubcategoryViewModel>


<div class="d-flex flex-row flex-wrap">
    @foreach (var categorySub in Model)
    {
        <div class="col-lg-3 col-md-4 mb-4 text-wrap">
            <a class="def-link-text" href="@Url.Action("Services","Global", new {IdSubcategory = categorySub.ID})">@categorySub.Name</a>
        </div>
    }
</div>