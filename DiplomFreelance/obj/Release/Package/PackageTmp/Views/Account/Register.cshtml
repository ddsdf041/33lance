@{
    ViewBag.Title = "Регистрация";
}

    <div class="container">
        <h2 class="text-center mt-4">@ViewBag.Title</h2>
        <ul class="nav nav-tabs" id="tabReg" role="tablist">
            <li class="nav-item">
                <a class="nav-link active" id="customer-tab" data-toggle="tab" href="#customerReg" role="tab" aria-controls="Customer" aria-selected="true">Заказчик</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="executor-tab" data-toggle="tab" href="#executorReg" role="tab" aria-controls="Executor" aria-selected="false">Исполнитель</a>
            </li>
        </ul>
        <div class="tab-content" id="tabRegContent">

            <div class="tab-pane fade show active" id="customerReg" role="tabpanel" aria-labelledby="customer-tab">
                @Html.Partial("_CustomerRegistrationPartial")
            </div>

            <div class="tab-pane fade" id="executorReg" role="tabpanel" aria-labelledby="executor-tab">
                @Html.Partial("_ExecutorRegistrationPartial")
            </div>

        </div>
    </div>
